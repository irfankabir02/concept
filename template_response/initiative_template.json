  "example_scenario": {
    "title":"NBA Scouting Agent (Realtime‑API demo)",
    "purpose":"Show how to split a monolithic data‑heavy function into focused tools, keep the context window small, and guide the model with hint prompts.",
    "initial_tool_definition": {
      "session.update": {
        "tools": [
          {
            "type":"function",
            "name":"searchDraftProspects",
            "description":"Search NBA draft prospects by position, year and optional max ranking. Returns a **compact list** of general statistics only.",
            "parameters":{
              "type":"object",
              "properties":{
                "position":{"type":"string","enum":["Point Guard","Shooting Guard","Small Forward","Power Forward","Center","Any"]},
                "year":{"type":"integer"},
                "maxMockDraftRanking":{"type":"integer","description":"Upper bound for mock ranking (e.g., 10 for top‑10)"},
                "limit":{"type":"integer","default":5,"minimum":1,"maximum":10}
              },
              "required":["position","year"]
            }
          },
          {
            "type":"function",
            "name":"getProspectDetails",
            "description":"Retrieve the full record for a single prospect. Caller can toggle optional sections to keep payload small.",
            "parameters":{
              "type":"object",
              "properties":{
                "playerName":{"type":"string"},
                "year":{"type":"integer"},
                "includeAgentInfo":{"type":"boolean","default":false},
                "includeStats":{"type":"boolean","default":true},
                "includeScoutingReport":{"type":"boolean","default":true}
              },
              "required":["playerName","year"]
            }
          }
        ],
        "tool_choice":"auto"
      }
    },

    "lightweight_search_example_response": {
      "status":{"code":200,"message":"SUCCESS"},
      "found":4274,
      "offset":0,
      "limit":5,
      "data":[
        {
          "prospectId":10001,
          "firstName":"Jalen",
          "lastName":"Storm",
          "position":"PG",
          "collegeTeam":"Springfield Tigers",
          "conference":"Big West",
          "mockDraftRanking":5,
          "averagePoints":21,
          "averageRebounds":4,
          "averageAssists":7,
          "heightFeet":6,
          "heightInches":4,
          "weightPounds":205
        }
      ]
    },

    "full_detail_example_call": {
      "type":"function",
      "name":"getProspectDetails",
      "arguments":{
        "playerName":"Jalen Storm",
        "year":2025,
        "includeAgentInfo":true,
        "includeStats":true,
        "includeScoutingReport":true
      }
    },

    "full_detail_example_response": {
      "status":{"code":200,"message":"SUCCESS"},
      "prospectId":10001,
      "playerName":"Jalen Storm",
      "position":"PG",
      "height":"6'4\"",
      "weightPounds":205,
      "collegeTeam":"Springfield Tigers",
      "conference":"Big West",
      "teamRanking":12,
      "coach":{"coachId":987,"coachName":"Marcus Reed"},
      "stats":{
        "gamesPlayed":32,
        "minutesPerGame":35,
        "fieldGoalPct":47,
        "threePtPct":39,
        "freeThrowPct":86,
        "pointsPerGame":21,
        "reboundsPerGame":4,
        "assistsPerGame":7,
        "stealsPerGame":2,
        "blocksPerGame":0
      },
      "scouting":{
        "strengths":["Court vision","Clutch shooting"],
        "areasForImprovement":["Defensive consistency"]
      },
      "draftProjection":{"mockDraftRanking":5,"lotteryPickProbability":88},
      "media":{"highlightReelUrl":"https://example.com/highlights/jalen-storm"},
      "agent":{"agentName":"Rick Allen","agency":"Elite Sports Management","contactEmail":"rallen@elitesports.com"}
    },

    "guiding_principles": [
      "Split large functions into focused tools (search vs. detail).",
      "Return only the fields needed for the next step – use `limit` and optional booleans to prune payload.",
      "Flatten hierarchical data; keep a single‑level JSON where possible.",
      "Round numbers and convert units (e.g., weight to nearest 5 lb, height to spoken form).",
      "Periodically summarise the conversation to keep the rolling context < 16 k tokens.",
      "Re‑inject the system prompt / tool list via `session.update` if the model forgets its role."
    ],

    "hint_prompt_template": "\nParse NBA prospect data and provide a concise, engaging response.\n\nGeneral Guidelines\n- Act as an NBA scouting expert.\n- Highlight key strengths and notable attributes.\n- Use conversational language; mention each attribute only once.\n- Ignore IDs, URLs, exact birth dates, and agent contact unless explicitly asked.\n\nPlayer Details\n- State height conversationally (e.g., \"six‑foot‑four\").\n- Round weight to the nearest 5 lb.\n\nStats & Draft Info\n- Round all stats to whole numbers.\n- Refer to the draft ranking with phrases like \"top‑five pick\".\n\nExperience\n- Mention class year (freshman, sophomore…) if available.\n- Include hometown city & state.\n\nExamples\n- \"Jalen Storm, a dynamic six‑foot‑four point guard from Springfield, Illinois, averages 21 points per game.\"\n- \"Known for his clutch shooting, he's projected as a top‑five pick.\"\n\nImportant: Respond strictly using the supplied data; do not fabricate information.\n"
  }
